apiVersion: apps/v11
kind: "Deployment"
metadata:
  name: backend-svc2
  labels:
    app: backend-svc2
  namespace: test
spec:
  replicas: 1
  revisionHistoryLimit: 2
  selector:
    matchLabels:
      app: backend-svc2
  template:
    metadata:
      labels:
        app: backend-svc2
    spec:
      containers:
      - name: backend-svc2
        image: nginx:1.14.2
        ports:
        - containerPort: 80
        resources:
          limits:
            cpu: "150m"
            memory: "250Mi"
          requests:
            cpu: "150m"
            memory: "250Mi"
